@model Accounts.Models.SelectListModel

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@Html.ActionLink("Add User","Add","User")

<table class="table table-bordered">
    @foreach (var item in Model.ListItems)
    {
        <tr><td>@item.Text</td><td>@Html.ActionLink("Edit", "Update", new { @id = item.Value })</td></tr>
    }
</table>
